      <xs:element minOccurs="0" name="RequestedDate" type="xs:dateTime" />
      <xs:element minOccurs="0" name="SendCopyOfChat" type="xs:boolean" />
      <xs:element minOccurs="0" name="VisitorEmail" nillable="true" type="xs:string" />
      <xs:element minOccurs="0" name="VisitorIp" nillable="true" type="xs:string" />
      <xs:element minOccurs="0" name="VisitorName" nillable="true" type="xs:string" />
