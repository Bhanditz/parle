using System;
using System.Collections.Generic;
using System.Linq;
using System.Text;
using System.Configuration.Provider;

namespace LiveChat.Providers
{
    public class OperatorProviderCollection : ProviderCollection
    {
        // Return an instance of OperatorProvider
        // for a specified provider name
        new public OperatorProvider this[string name]
        {
            get { return (OperatorProvider)base[name]; }
        }
    }

}
